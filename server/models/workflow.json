{
  "name": "Workflow",
  "plural": "Workflows",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false,
      "validateUpsert": true,
      "silenceWarnings": false
    }
  },
  "indexes": {
    "project_unique_constraint": {
      "keys": {
        "projectId": 1,
        "name": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false
    },
    "json": {
      "type": "object",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "project": {
      "type": "belongsTo",
      "model": "Project",
      "foreignKey": "projectId"
    },
    "tasks": {
      "type": "hasMany",
      "model": "Task",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
